{
  "name": "nekohack-ui",
  "version": "2.0.0",
  "description": "",
  "private": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "esm",
    "lib"
  ],
  "license": "MIT",
  "author": "jiyuujin",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiyuujin/nekohack-ui.git"
  },
  "bugs": {
    "url": "https://github.com/jiyuujin/nekohack-ui/issues"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6001 -c .storybook",
    "build-storybook": "build-storybook",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.build.json",
    "build": "npm run clear && run-s build:lib build:esm",
    "clear": "rm -rf esm/ lib/",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch",
    "lint": "eslint ./ --ext .ts,.tsx",
    "lint:fix": "eslint ./ --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.17",
    "@babel/preset-react": "7.12.7",
    "@babel/preset-typescript": "7.12.13",
    "@nekohack/eslint-config-react": "0.3.0",
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-actions": "6.1.18",
    "@storybook/react": "6.1.16",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.28",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.9.0",
    "babel-jest": "26.6.3",
    "css-loader": "5.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "fibers": "5.0.0",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "1.3.6",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "purgecss-webpack-plugin": "4.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "1.32.7",
    "sass-loader": "10.1.1",
    "styled-components": "5.2.1",
    "ts-jest": "26.5.1",
    "ts-loader": "8.0.17",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0",
    "react-dom": "^16.13.1 || ^17.0.0",
    "styled-components": "^5.1.1"
  },
  "jest": {
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}"
    ]
  }
}
