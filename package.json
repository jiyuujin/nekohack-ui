{
  "name": "nekohack-ui",
  "version": "0.2.3",
  "description": "",
  "private": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "esm",
    "lib"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6001 -c .storybook",
    "build-storybook": "build-storybook",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.build.json",
    "build": "npm run clear && run-s build:lib build:esm",
    "clear": "rm -rf esm/ lib/",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch",
    "lint": "eslint ./ --ext .ts,.tsx",
    "lint:fix": "eslint ./ --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@nekohack/eslint-config-react": "^0.3.0",
    "@storybook/addon-actions": "^6.0.10",
    "@storybook/react": "^6.0.10",
    "@types/node": "^14.0.19",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-jest": "^26.3.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.4.0",
    "fibers": "^5.0.0",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "purgecss-webpack-plugin": "^2.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.2.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "jest": {
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}"
    ]
  }
}
